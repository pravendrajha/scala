default --- ---

Defined member as private then can't access out side the class

class User( var name :String , var age:Int)
{

  def printName(): Unit =
  {
    print(name )
  }

}
object SampleData {


  def main(args: Array[String]): Unit = {

    var user = new User("max"  ,20 )

    print(user.name)
    print(user.age)
    user.printName()

  }
}
